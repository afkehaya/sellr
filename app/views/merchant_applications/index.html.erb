<p id="notice"><%= notice %></p>

<h1>Listing Merchant Applications</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Company</th>
      <th>Phone number</th>
      <th>Website</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @merchant_applications.each do |merchant_application| %>
      <tr>
        <td><%= merchant_application.first_name %></td>
        <td><%= merchant_application.last_name %></td>
        <td><%= merchant_application.company %></td>
        <td><%= merchant_application.phone_number %></td>
        <td><%= merchant_application.website %></td>
        <td><%= link_to 'Show', merchant_application %></td>
        <td><%= link_to 'Edit', edit_merchant_application_path(merchant_application) %></td>
        <td><%= link_to 'Destroy', merchant_application, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Merchant application', new_merchant_application_path %>
